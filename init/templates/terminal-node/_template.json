{
  "params": [
    {
      "name": "name",
      "prompt": "name",
      "validation": "(name) => /^[@/a-z0-9-_]+$/.test(name)"
    },
    {
      "name": "main",
      "default": "index.js",
      "prompt": "main",
      "validation": "(value) => value.endsWith('.js')",
      "msg": "must have a .js file extension"
    },
    {
      "name": "license",
      "default": "Apache-2.0",
      "prompt": "license"
    }
  ]
}
